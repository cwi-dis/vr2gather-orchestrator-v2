{
  "compilerOptions": {
    "outDir": ".",
    "noImplicitAny": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "sourceMap": true,
    "target": "es6",
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "baseUrl": ".",
    "paths": {
      "typings/*": [
        "util/typing/*"
      ]
    }
  },
  "include": [
    "./**/*.ts"
  ],
  "exclude": [
    "./node_modules/**/*",
  ]
}
